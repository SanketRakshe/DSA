

code:

#include<bits/stdc++.h>
using namespace std;
void PrintArray(int arr[], int n) {
    for(int i=0; i<n; i++) {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}
void Selectionsort(int arr[], int n) {
    for(int i=0; i<n-1; i++) {
        int minIndex = i;
        for(int j=i+1; j<n; j++) {
            if(arr[j] < arr[minIndex]) {
                minIndex = j;
            }
        }
        swap(arr[minIndex], arr[i]);
    }
}
int main() {
    int arr[6] = {6,2,8,4,10,1};
    PrintArray(arr, 6);
    Selectionsort(arr, 6);
    PrintArray(arr, 6);

    return 0;
}

/*
Time Complexity:
Worst Case = O(N2)
Best Case = O(N2)
Avg Case = O(N2)

Space Complexity = O(1)
*/
